{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard", "standard-jsx", "standard-react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-shadow": "warn",
    "react/prop-types": "off"
  }
}